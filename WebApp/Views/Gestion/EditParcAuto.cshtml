@model Domain.Parc_auto

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Edition Parc automobile
            </div>
            <div class="panel-body">



                <div class="panel panel-default">


                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                    @Html.HiddenFor(model => model.Id_parc)
                        @Html.HiddenFor(model => model.idDelegation)
                        @Html.HiddenFor(model => model.idBatiment)
                        <div class="form-horizontal">
                            <h4>Parc automobile</h4>
                            <hr />
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            <div class="form-group">
                                <label class="control-label col-lg-4" style="font-style:oblique;">Nom</label>
                                <div class="col-lg-6">

                                    @Html.EditorFor(model => model.Libelle, new { @class = "form-control", required = "" })
                                    @Html.ValidationMessageFor(model => model.Libelle, String.Empty, new { @class = "test" })
                                </div>
                            </div>




                            <div class="form-group">
                                <label class="control-label col-lg-4" style="font-style:oblique;">Délégation </label>
                                <div class="col-lg-6">
                                    @Html.TextBoxFor(model => model.Batiment.Delegation.libelle, new { @class = "form-control", required = "", disabled = "disabled" })
                                    @Html.ValidationMessageFor(model => model.Batiment.Delegation.libelle, String.Empty, new { @class = "test" })
                                    @*@Html.DropDownListFor(model => model.idDelegation, (SelectList)ViewData["delegation"], "--Choisir--", new { @class = "form-control", required = "" })*@
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-lg-4" style="font-style:oblique;">Bâtiment </label>
                                <div class="col-lg-6">
                                    @Html.TextBoxFor(model => model.Batiment.description, new { @class = "form-control", required = "", disabled = "disabled" })
                                    @Html.ValidationMessageFor(model => model.Batiment.description, String.Empty, new { @class = "test" })
                                    @*@Html.DropDownListFor(model => model.idBatiment, (SelectList)ViewData["batiment"], "--Choisir--", new { @class = "form-control", required = "" })*@
                                </div>
                            </div>

                        </div>
                        <div style="text-align: center;">
                            <input type="submit" value="Valider" class="btn btn-default" />

                            <button type="reset" class="btn btn-white">@Html.ActionLink("Annuler", "GetParcAuto")</button>

                        </div>

                    }
                </div>



            </div>
        </div>
    </div>
</div>
