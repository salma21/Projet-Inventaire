@model IEnumerable<Domain.MouvementBien>


<!-- Start Row -->
<div class="row">
    <!-- Start Panel -->
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Liste des Mouvements Biens
            </div>
            <div class="panel-body table-responsive">
                <table id="example0" class="table display">
                    <thead>
                        <tr>
                            <th>Code Bien</th>
                            <th>Nom Bien</th>
                            <th>Batiment</th>
                            <th>Étage</th>
                            <th>Désignation</th>
                            @*<th>Matricule</th>
                            <th>Nom</th>
                            <th>Prénom</th>*@
                            <th>Date derniére affectation</th>
                            <th>Date prochaine affectation</th>
                            <th>Date renouvellementt</th>
                            <th>Date retour prévue</th>
                            <th>Date sortie</th>
                            <th>Action</th>
                        </tr>
                    </thead>



                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td class="center">@item.Bien.Code_a_barre</td>
                                <td class="center">@item.Bien.Designation</td>
                                <td class="center">@item.Bien.Etage.Batiment.description</td>
                                <td class="center">@item.Bien.Etage.description</td>
                                <td class="center">@item.Nom</td>
                                @*<td class="center">@item.Bien.Personnel.Matricule</td>
                                <td class="center">@item.Bien.Personnel.nom</td>
                                <td class="center">@item.Bien.Personnel.prenom</td>*@
                                <td class="center">@item.Date_derniere_affectation</td>
                                <td class="center">@item.Date_prochaine_affectation</td>
                                <td class="center">@item.Date_renouvellement_prevue</td>
                                <td class="center">@item.Date_retour_prevue</td>
                                <td class="center">@item.Date_sortie</td>


                                <td>
                                    @if (@Session["Role"].ToString() == "ADMIN")
                                    {
                                        <a href="@Url.Action("Edit", new { id = item.Id_mouvementB } )">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<i class="fa fa-edit" title="Modifer"></i></a>
                                    }
                                </td>

                            </tr>


                        }
                    </tbody>
                </table>
                <br /><p>
@if (@Session["Role"].ToString() == "ADMIN")
{
                    @Html.ActionLink("Ajouter un nouveau Mouvement Bien", "CreateMouvementBien")
}
                </p>

            </div>
        </div>
    </div>


</div>





<script>
    $(document).tooltip();
</script>