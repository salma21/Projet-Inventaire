@model IEnumerable<Domain.Trace>
<!-- Start Row -->
<div class="row">
    <!-- Start Panel -->
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-title">
                Liste Des Bureaux
                <hr />
            </div>
            <div class="panel-body table-responsive">
                <table id="example0" class="table display">
                    <thead>
                        <tr>
                            <th>Utlisateur</th>
                            <th>Action</th>
                            <th>Table</th>

                            <th>Champ</th>
                            <th>Date</th>

                        
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td class="center">@item.Users</td>
                                <td class="center">@item.Actions</td>
                                <td class="center">@item.Tables</td>

                                <td class="center">@item.Champs</td>
                                <td class="center">@item.Dates</td>


                            </tr>


                        }
                    </tbody>
                </table>
             

            </div>



        </div>



    </div>
</div>
